{
  "name": "snickers-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npm run migration:refresh:test && NODE_ENV=test npm run db:seed && NODE_ENV=test nyc --reporter=html --reporter=text mocha --timeout 20000 -r @babel/register ./src/test/**/*.test.js --exit",
    "start": "node dist/app.js",
    "dev": "nodemon --exec babel-node src/app.js",
    "build": "rm -rf dist/ && babel src/ -d dist/",
    "migration": "sequelize db:migrate",
    "migration:rollback": "sequelize db:migrate:undo:all",
    "migration:refresh": "npm run migration:rollback && npm run migration",
    "migration:refresh:test": "NODE_ENV=test npm run migration:rollback && NODE_ENV=test npm run migration",
    "db:seed": "sequelize db:seed:all --debug",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "nyc": "^15.1.0",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.11"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/node": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/register": "^7.13.8",
    "chai": "^4.3.3",
    "chai-http": "^4.3.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.21.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "mocha": "^8.3.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^2.0.7"
  }
}
